/* pages/myadmin/faceback/facebacks.wxss */
.container{
  background: #F0EFF5;
}
.editor{
  width: 100%;
  margin-top: 5%;
}

.editor-text{
  width: 100%;
  height: 300rpx;
  background-color: #FFFFFF;
  padding: 10rpx;
  font-size: 34rpx;
  font-family: "Microsoft YaHei UI";
  border-bottom: 2rpx solid #E7E7EB;
}

.contact{
  width: 100%;
  height: 80rpx;
  background: #FFFFFF;
  display: flex;
  justify-content: center;
  margin-top: 5%;
  border-top: 2rpx solid #E7E7EB;
  border-bottom: 2rpx solid #E7E7EB;
  margin-bottom: 5%;
}
.contact text{
  width: 30%;
  height: 80rpx;
  padding-left: 4%;
  line-height: 80rpx;
  font-size: 34rpx;
  font-family: "Microsoft YaHei UI";
}
.contact textarea{
  width: 70%;
  height: 80rpx;
  padding-top: 20rpx;
  line-height: 80rpx;
  font-size: 34rpx;
  font-family: "Microsoft YaHei UI";
}
.navigator{
  width: 90%;
  margin: 5%;
}