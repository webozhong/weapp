.container{
  margin-left: 5%;
  color: #666;
  width: 90%;
}

.logo {
  width: 200rpx;
  height: 200rpx;
  border: 1rpx solid #dfdfdf;
  border-radius: 10rpx;
  background-color: #fff;
  margin: 0 auto;
  margin-top: 80rpx;
  text-align: center;
  line-height: 200rpx;
}

.logo-image{
  width: 160rpx;
  height: 160rpx;
  vertical-align: middle;
}

.name{
  text-align: center;
  font-size: 50rpx;
  margin-top: 50rpx;
  font-family: "微软雅黑";
  color: black;
   
}

.about{
  font-size: 30rpx;
  text-align: center;
  margin-top: 80rpx;
}

.about-text{
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  display: block;
  font-family: "Microsoft YaHei UI";
  font-size: 34rpx;
  color: black;
}

.bottom-container{
  font-size: 25rpx;
  text-align: center;
  padding: 30rpx 0;
}
text{
  display: block
}